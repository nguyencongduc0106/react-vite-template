{
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "env-cmd -f .env/.env.development vite dev",
    "start:stag": "env-cmd -f .env/.env.staging vite dev",
    "start:prod": "env-cmd -f .env/.env.production vite dev",
    "build:dev": "env-cmd -f .env/.env.development vite build",
    "build:stag": "env-cmd -f .env/.env.staging vite build",
    "build:prod": "env-cmd -f .env/.env.production vite build",
    "preview": "vite preview",
    "commit": "npx cz",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "check-types": "tsc --noEmit --pretty",
    "postinstall": "husky install || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.54.1",
    "@tanstack/react-router": "^1.48.1",
    "clsx": "^2.1.1",
    "env-cmd": "^10.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@tanstack/router-plugin": "^1.47.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "check-types": "^11.2.3",
    "commitizen": "^4.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.0.1",
    "husky": "9.0.11",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "vite": "^5.4.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
